    <section class="page-section bg-light" id="wolontariat">
        <div class="container">
            <div class="text-center">
                <h2 class="section-heading text-uppercase">Wolontariat</h2>
                <h3 class="section-subheading text-muted">Dołącz do nas!<br>Odezwiemy się do ciebie za pośrednictwem
                    maila lub telefonicznie</h3>
                <form id="vol_form">
                    <!-- układ 2 kolumnowy z miejscami na imie i nazwisko -->
                    <div class="row mb-4">
                        <div class="col">
                            <div class="form-outline">
                                <input type="text" id="name" required
                                    pattern="[A-Za-ząćęłńóśźżĄĘŁŃÓŚŹŻ ]{3,}[^a-zA-ZąćęłńóśźżĄĘŁŃÓŚŹŻ]{0,}"
                                    placeholder="Podaj swoje imię" class="form-control" />
                                <label class="form-label" for="name">Imie</label>
                            </div>
                        </div>

                        <div class="col">
                            <div class="form-outline">
                                <input type="text" id="surname" required pattern="[A-Za-ząćęłńóśźżĄĘŁŃÓŚŹŻ]{3,}"
                                    placeholder="Podaj swoje nazwisko" class="form-control" />
                                <label class="form-label" for="surname">Nazwisko</label>
                            </div>
                        </div>
                    </div>

                    <!-- układ 2 kolumnowy z miejscami na mail i numer telefonu -->
                    <div class="row mb-4">
                        <div class="col">
                            <div class="form-outline">
                                <input type="email" id="mail" class="form-control" required
                                    placeholder="Podaj swój adres mail" />
                                <label class="form-label" for="mail">E-mail</label>
                            </div>
                        </div>

                        <div class="col">
                            <div class="form-outline">
                                <input type="tel" id="phone" required pattern="[0-9]{3}-[0-9]{3}-[0-9]{3}"
                                    placeholder="XXX-XXX-XXX" class="form-control" />
                                <label class="form-label" for="phone">Numer telefonu</label>
                            </div>
                        </div>
                    </div>

                    <!-- miejsce na informacje o sobie -->
                    <div class="form-outline">
                        <textarea class="form-control" id="about" required rows="2"></textarea>
                        <label class="form-label" for="about">Napisz nam coś o sobie</label>
                    </div>

                    <!-- wysłanie formularza -->
                    <button id="form_submit" type="button" onclick="saveVolForm()"
                        class="btn btn-primary text-dark btn-block mb-4">Złóż formularz</button>
                </form>
            </div>
        </div>
        </div>
    </section>

