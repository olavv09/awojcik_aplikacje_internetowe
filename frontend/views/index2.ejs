    <script type="text/babel">
        const dogs = []
        const images = []
        const gender = []
        axios.get('/dogs/all')
            .then(function (response) {
                // handle success
                console.log(response.data);
                for (let i = 0; i < response.data.length; i++) {
                    dogs.push(response.data[i].name)
                    gender.push(response.data[i].sex)
                    images.push(response.data[i].photo)
                }

            }).then(function () {
                ReactDOM.render(<Headline />,
                    document.getElementById("aaaxd"))
            });
        class Headline extends React.Component {
            render() {

                let counter = 0
                //let i1 = "frontend/assets/img/portfolio/pies"
                //let i2 = ".jpg"
                return (<div class="row">
                    {dogs.map(dog => <div class="col-lg-4 col-sm-6 mb-4">
                        <div class="portfolio-item">
                            <a class="portfolio-link" data-bs-toggle="modal" href="#portfolioModal1">
                                <div class="portfolio-hover">
                                    <div class="portfolio-hover-content"><i class="fas fa-plus fa-3x"></i></div>
                                </div>
                                <img class="img-fluid" src={images[counter]} alt="..." />
                            </a>
                            <div class="portfolio-caption">
                                <div class="portfolio-caption-heading">{dog}</div>
                                <div class="portfolio-caption-subheading text-muted">{gender[counter++]}</div>
                            </div>
                        </div>
                    </div>)}</div>)

            }
        }


    </script>






